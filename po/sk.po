#: ../debian/tmp/usr/lib/python3.9/dist-packages/apport/ui.py:306
#: ../debian/tmp/usr/lib/python3.9/dist-packages/apport/ui.py:1458
#: ../apport/ui.py:306 ../apport/ui.py:1458
#, python-format
