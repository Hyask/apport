#: ../bin/apport-cli.py:304 ../gtk/apport-gtk.ui.h:22 ../kde/apport-kde.py:349
msgid "Uploading problem information"
msgstr "Tải lên thông tin về vấn đề"

#: ../bin/apport-cli.py:305
msgid ""
"The collected information is being sent to the bug tracking system.\n"
"This might take a few minutes."
msgstr ""
"Thông tin thu thập đang được gửi tới hệ thống kiểm duyệt lỗi.\n"
"Côn việc này có thể mất vài phút."

#: ../bin/apport-cli.py:355
msgid "&Done"
msgstr "Đã &xong"

#: ../bin/apport-cli.py:361
msgid "none"
msgstr "không"

#: ../bin/apport-cli.py:362
#, python-format
msgid "Selected: %s. Multiple choices:"
msgstr "Đã chọn: %s. Nhiều lựa chọn:"

#: ../bin/apport-cli.py:378
msgid "Choices:"
msgstr "Các lựa chọn:"

#: ../bin/apport-cli.py:392
