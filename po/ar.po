msgstr "برنامج مجهول"

#: ../debian/tmp/usr/lib/python2.7/dist-packages/apport/ui.py:250
#: ../apport/ui.py:250
#, python-format
msgid "Sorry, the program \"%s\" closed unexpectedly"
