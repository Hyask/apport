#!/bin/sh -e

# clean up .pyc so that the programs actually use the new library in the python
# path
if dpkg --compare-versions "$2" le-nl 0.17; then
    rm -f /usr/share/apport/apport_utils.pyc
fi

# clean away unnecessary rc symlinks on upgrade

if dpkg --compare-versions "$2" le-nl 0.21; then
    rm -f /etc/rc0.d/K01apport /etc/rc6.d/K01apport
fi

#DEBHELPER#
